/* 搜索框 */
.search{
  width: 100%;
  height: 100rpx;
  background-color: rgb(0,178,106);
  border: 1rpx rgb(0,178,255) solid;
}
.search .search-btn{
  width: 80%;
  height: 70rpx;
  background-color: white;
  border-radius: 20rpx;
  line-height: 70rpx;
  font-size: 32rpx;
  text-align:center;
  margin:15rpx 0 0 10%;
}